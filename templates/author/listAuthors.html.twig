<table border="1" align="center">

    <caption>List of authors</caption>

    <tr>

        <td>Id</td>
        <td>Username</td>
        <td>Email</td>
    </tr>



    {% for a in list %}
    <tr>
        <td>{{a.id}}</td>
        <td>{{a.username}}</td>
        <td>{{a.email}}</td>
    </tr>

    {% endfor %}
</table>